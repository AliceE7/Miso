<script src="https://code.iconify.design/1/1.0.7/iconify.min.js"></script>
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
<link rel="icon" type="image/png" href="<%= bot.user.displayAvatarURL({size: 128}) %>">

    <nav class="navbar navbar-expand-lg justify-content-center navbar-dark" style="background-color: #000000;">
        <div class="container">
            <a class="navbar-brand" href="/" style="text-decoration: none;color:white">
                              <img src="<%= bot.user.displayAvatarURL({size: 2048}) %>" alt="Logo" 
                class="d-inline-block align-top"
                >
            </a> <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                <li class="nav-item">
                    <a href="/commands">commands</a>
                </li>
                <li class="nav-item">
                    <a href="https://discord.com/api/oauth2/authorize?client_id=<%= bot.user.id %>&permissions=8&scope=bot%20applications.commands" target="_blank">
                        <b>invite</b>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="/dashboard"><b>dashboard</b></a>
                </li>
            </ul>
            <ul class="nav navbar-right">
                <li class="nav-item" >
                    <% if(user && user.id) { %>
                        <button class="btn btn-danger btn-sm" 
                        style="letter-spacing: 1px;font-weight: 460;width: 100%;"
                        onclick="location.href='/logout'" type="button">Logout</button>
                    <% }else { %>
                        <button class="btn btn-success btn-sm" 
                        style="letter-spacing: 1px;font-weight: 450;width: 100%;"
                        onclick="location.href='/login'" type="button"><span class="iconify" data-icon="simple-icons:discord"></span> Login</button>
                    <% }%>
                </li>
            </ul>
        </div>
    </nav>

<script>
    const urlParams = new URLSearchParams(window.location.search);
    let myParam = urlParams.get('error');
    if(myParam) alert(myParam);
    myParam = urlParams.get('success');
    if(myParam) alert(myParam);
</script>